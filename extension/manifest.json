{
  "name": "Carte - leboncoin.fr",
  "description": "Place les annonces du site leboncoin.fr sur une carte",
  "version": "0.2",
  "permissions": [
    "https://www.leboncoin.fr/*"
  ],
  "icons": {
    "16": "assets/16.png",
    "128": "assets/128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.leboncoin.fr/*"],
      "js": ["main.js"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
